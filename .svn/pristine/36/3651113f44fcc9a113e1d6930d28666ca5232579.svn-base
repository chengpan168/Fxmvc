@CHARSET "UTF-8";
.baseAddPane{
	-fx-background-color:green ;
}
.baseAddPane title{
	-fx-background-color:green ;
}

.baseAddPaneTop{
	-fx-background-image: url("img/addtop.png");
	-fx-background-repeat: repeat-x;
	-fx-border-width : 1 ;
	-fx-border-color : rgb(100, 100, 100, 0.7);
}

.baseAddPaneTitle{
	-fx-background-image: url("img/add.png");
	-fx-background-repeat: repeat-x;
	-fx-vpos: bottom  ;
	-fx-border-width : 1 ;
	-fx-border-color : rgb(20, 20, 100, 0.5);
	-fx-font-size : 11 ;
}

.addScrollPane{
	-fx-border-width : 1 ;
	-fx-border-color : rgb(20, 20, 100, 0.1) , transparent  , transparent , transparent;
	-fx-background-color:rgb(235, 235, 235, 0.5);
}
.addScrollPane:focused{
	-fx-background-color:
		transparent ,
		rgb(235, 235, 235, 0.5) ,
		transparent ;
}   
	
.addTextField{
	-fx-prompt-text-fill:
        rgb(128,128,128);
    -fx-background-color:
    	rgb(235, 235, 235, 0.5),
        rgb(100, 100, 200, 0.4),
        rgb(255, 255, 255);
}
.addTextField:focused{
	-fx-prompt-text-fill:
        rgb(128,128,128);
    -fx-background-color:
    	rgb(150, 150, 220, 0.5),
        rgb(0, 0, 0, 0.4),
        rgb(255, 255, 255);
}

.addTextArea{
	-fx-prompt-text-fill:
        rgb(128,128,128);
    -fx-background-color:
    	rgb(235, 235, 255, 0.5),
        rgb(100, 100, 200, 0.4),
        rgb(255, 255, 255);
}
.addTextArea:focused{
	-fx-prompt-text-fill:
        rgb(128,128,128);
    -fx-background-color:
    	rgb(150, 150, 220, 0.5),
        rgb(0, 0, 0, 0.4),
        rgb(255, 255, 255);
}

.addChoiceBox{
	-fx-prompt-text-fill:
        rgb(128,128,128);
    -fx-background-color:
    	rgb(235, 235, 235, 0.5),
        rgb(100, 100, 200, 0.4),
        rgb(255, 255, 255);
}